{{>layout/header}}
<h1>자산 검색</h1>
<div class="col-md-12">
    <div class="col-md-4">
        <form>
            <div class = "form-group">
                <input type = "text" id="stockWord" placeholder="자산명 또는 티커를 입력하세요.">
                <button type="button" id="stock-search">Search</button> <ul id="stockList"></ul>
                <script type="text/template" id="template">
                {{#stock}}
                    <li>
                        {{name}}
                    </li>
                {{/stock}}
                {{^stock}}
                <li>
                    결과가 없습니다 제발좀
                </li>
                {{/stock}}
                </script>
            </div>
        </form>
    </div>
</div>
{{>layout/script}}
{{>layout/footer}}